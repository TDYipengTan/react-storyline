{
  "presets": [
    ["@babel/env", { "targets": { "node": "current" } }],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    ["@cobalt/babel-plugin-data-attributes", {
      "overrides": {
        "WeekDays": { "process": false },
        "Weekdays": { "process": false },
        "Day": { "process": false },
        "Days": { "process": false },
        "DayInner": { "process": false }
      }
    }]
  ]
}

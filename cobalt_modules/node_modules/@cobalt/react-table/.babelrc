{
  "presets": [
    ["@babel/env", { "targets": { "node": "current" } }],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    ["@cobalt/babel-plugin-data-attributes", {
      "overrides": {
        "Body": { "process": false },
        "DragHandle": { "process": false },
        "ExpandableRow": { "process": false },
        "Head": { "process": false },
        "HeadCell": { "process": false },
        "Cell": { "process": false },
        "Row": { "process": false }
      }
    }]
  ]
}
